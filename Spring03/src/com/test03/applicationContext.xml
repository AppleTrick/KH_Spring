<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
	
	<!-- 홍길동, 30000, 기술 -->
	<bean id="hong" class="com.test03.Engineer">
		<constructor-arg index="0" value="홍길동"></constructor-arg>
		<constructor-arg index="1" value="25000"></constructor-arg>
		<property name="dept" value="기술"></property>
	</bean>
	<!-- 이순신, 30000, 개발 -->
	<bean id="lee" class="com.test03.Developer">
		<constructor-arg index="0" value="이순신"></constructor-arg>
		<constructor-arg index="1" value="30000"></constructor-arg>
		<property name="dept" value="개발"></property>
	</bean>

</beans>
